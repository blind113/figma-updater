{
  "source": ["tokens/**/*.json"],
  "preprocessors": ["tokens-studio"],
  "excludeParentKeys": true,
  "platforms": {
    "css": {
      "transformGroup": "css",
      "transforms": ["name/kebab", 'shadow/css'],
      "files": [
        {
          "destination": "css/variables.css",
          "format": "css/variables"
        }
      ]
    }
  }
}
 
